    "react-c3js": "^0.1.20",
    "react-dom": "17.0.2",
    "react-tsparticles": "^1.35.3",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@types/react": "17.0.27",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "events": "^3.3.0",
    "jest": "^27.4.3",
    "typescript": "4.4.3"
