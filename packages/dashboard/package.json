    "@sidetree/cas": "^0.2.6-unstable.6",
    "@sidetree/cas-ipfs": "^0.2.6-unstable.6",
    "@sidetree/common": "0.2.6-unstable.6",
    "@sidetree/core": "^0.2.6-unstable.6",
    "@sidetree/crypto": "0.2.6-unstable.6",
